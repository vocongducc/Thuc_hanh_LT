print("sinh vien VO CONG DUC")
print("Ma so sinh vien:235752021610102")

class MyString:
    def __init__(self):
        self.string = ""
    
    def get_String(self):
        self.string = input("Nhập một chuỗi: ")
    
    def print_String(self):
        print(self.string.upper())


my_str = MyString()
my_str.get_String()
my_str.print_String()
