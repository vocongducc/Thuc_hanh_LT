def count_upper_lower(sentence):
upper_count = 0
lower_count = 0
for char in sentence:
if char.isupper():
upper_count += 1
  elif char.islower():
lower_count += 1
print (f" số chữ hoa là: (upper_count)") print (f"Số chữ thường là: (lower_count)")

