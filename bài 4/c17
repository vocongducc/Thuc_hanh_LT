print("Ma so sinh vien:235752021610102")

def sum of divisors (num):

total = 0

for i in range (1, num):

if num & i = 0:

total += i

return total

def is abundant (num):

return sum of divisors (num) > num

n=int(input ("Nhập số nguyên dương n: "))

print (f"Các số nguyên dương nhỏ hơn (n) có tổng các ước số lớn hơn chính nó là:"

for i in range(1, n):

if is abundant (i):

print(i)
